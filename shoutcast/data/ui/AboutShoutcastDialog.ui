    <property name="window_position">center</property>
    <property name="icon">../media/icon.png</property>
    <property name="type_hint">normal</property>
    <property name="has_separator">False</property>
    <property name="program_name">Shoutcast Demystified</property>
    <property name="version">0.22-public4</property>
    <property name="copyright" translatable="yes">Copyright (C) 2010 Aruna Hewapathirane &lt;aruna.hewapathirane@gmail.com&gt;</property>
    <property name="comments" translatable="yes">Shoutcast Radio 
Written using Python and Quickly</property>
    <property name="license"># Copyright (C) 2010 Aruna Hewapathirane &lt;aruna.hewapathirane@gmail.com&gt;
